algoritmo "semnome"
var
   jogador1, jogador2, jogador3 :inteiro

inicio
   escreval("Digite o jogador: ")
   aleatorio 1, 3
   jogador1 <- randi(1) + 1
   jogador2 <- randi(2) + 2
   jogador3 <- randi(3) + 3
   aleatorio off
   escreval("")
   escreval("jogador1: ", jogador1)
   escreval("jogador2: ", jogador2)
   escreval("jogador3: ", jogador3)
fimAlgoritmo