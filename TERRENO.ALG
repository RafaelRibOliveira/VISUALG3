Algoritmo "Terreno"

Var
   largura, comprimento, area_terreno: real


Inicio
      escreva("Entre com a largura do terreno: ")
      leia(largura)
      escreva("Entre com o comprimento: ")
      leia(comprimento)
area_terreno <- largura * comprimento
      escreva("Este terreno possui ", area_terreno, " metros quadrados")


Fimalgoritmo